<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ipsapp"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ips_app_icon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.ipsapp.EventosActivity"
            android:label="@string/title_activity_eventos" >
        </activity>
        <activity
            android:name="com.example.ipsapp.EventosActivity2"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.ipsapp.InfoEventoActivity"
            android:label="@string/title_activity_info_evento" >
        </activity>
        <activity
            android:name="com.example.ipsapp.MapasActivity"
            android:label="@string/title_activity_mapas" >
        </activity>
        <activity
            android:name="com.example.ipsapp.InfoAppActivity"
            android:label="@string/title_activity_info" >
        </activity>
    </application>

</manifest>
